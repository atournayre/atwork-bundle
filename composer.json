{
    "name": "atournayre/atwork-bundle",
    "description": "This bundle is an overlay to the Symfony framework.",
    "type": "symfony-bundle",
    "license": [ "MIT" ],
    "authors": [
        {
            "name": "atournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Atournayre\\Bundle\\AtWorkBundle\\": ""
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/config": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/routing": "^6.2",
        "symfony/security-http": "^6.2",
        "symfony/security-core": "^6.2",
        "api-platform/core": "^3.1",
        "doctrine/orm": "^2.14",
        "symfony/uid": "^6.2",
        "atournayre/assert": "^0",
        "atournayre/types": "^0",
        "symfony/security-bundle": "^6.2",
        "symfony/flex": "^2.2"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "endpoint": [
                "https://api.github.com/repos/atournayre/symfony-flex-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
